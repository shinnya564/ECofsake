
<div class="col-xs-12">
	<h2 class="page-header">会員一覧画面</h2>

	<%= paginate @admins %>
		<table class="table text-center">
			<thead>
				<tr>
					<th class="text-center">会員ID</th>
					<th class="text-center">メールアドレス</th>
				</tr>
			</thead>

			<% @admins.each do |admin| %>
				<tbody>
					<tr>
						<td><%= admin.id %></td>
						<td><%= link_to admin.email, admins_admin_path(admin.id), data: {"turbolinks" => false} %></td>
					</tr>
				</tbody>
			<% end %>
		</table>
	<%= paginate @admins %>
</div>