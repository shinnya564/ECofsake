
    <div class="col-xs-12">
        <h2>登録情報編集</h2><hr>
        <%= form_with(model:[@end_user], local: true) do |f| %>
                <div class="col-xs-3">
                    <div class="col-xs-6">
                        名前
                    </div>
                    <div class="col-xs-6">
                        (姓)
                    </div>
                </div>
                <div class="col-xs-9 field">
                    <%= f.text_field  :family_name, class: 'form-control field' %>
                </div>
                <div class="col-xs-3">
                    <div class="col-xs-6">
                    </div>
                    <div class="col-xs-6">
                        (名)
                    </div>
                </div>
                <div class="col-xs-9 field">
                    <%= f.text_field  :first_name, class: 'form-control' %>
                </div>
                <div class="col-xs-3">
                    <div class="col-xs-6">
                        フリガナ
                    </div>
                    <div class="col-xs-6">
                        (セイ)
                    </div>
                </div>
                <div class="col-xs-9 field">
                    <%= f.text_field  :kana_family_name, class: 'form-control field' %>
                </div>
                <div class="col-xs-3">
                    <div class="col-xs-6">
                    </div>
                    <div class="col-xs-6">
                        (メイ)
                    </div>
                </div>
                <div class="col-xs-9 field">
                    <%= f.text_field  :kana_first_name, class: 'form-control' %>
                </div>

                <div class="col-xs-3">
                    <div class="col-xs-9">
                        メールアドレス
                    </div>
                </div>

                <div class="col-xs-9 field">
                    <%= f.text_field  :email, class: 'form-control' %>
                </div>

                <div class="col-xs-3">
                    <div class="col-xs-9">
                        郵便番号(ハイフンなし)
                    </div>
                </div>
                <div class="col-xs-9 field">
                    <%= f.text_field  :postal_code, class: 'form-control' %>
                </div>

                <div class="col-xs-3">
                    <div class="col-xs-9">
                        住所
                    </div>
                </div>
                <div class="col-xs-9 field">
                    <%= f.text_field  :address, class: 'form-control' %>
                </div>

                <div class="col-xs-3">
                    <div class="col-xs-9">
                        電話番号
                    </div>
                </div>
                <div class="col-xs-9 field">
                    <%= f.text_field  :tel, class: 'form-control' %>
                </div>

                <div class="col-xs-3">
                </div>
                <div class="col-xs-9 field">
                    <%= f.submit "変更する", {class: "btn btn-success field"} %><br><br>
                    <%= link_to "戻る", :back, class: "btn btn-primary field" %>
                </div>
            </div>

        <% end %>
    </div>